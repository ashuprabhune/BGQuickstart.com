{
  "name": "bg-quickstart",
  "version": "1.0.1",
  "description": "The fastest way to pick a start player.",
  "main": "src/index.js",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@types/lodash-es": "^4.17.3",
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "framer-motion": "1.8.4",
    "lodash-es": "4.17.15",
    "postcss-cli": "^7.1.0",
    "preact": "^10.4.7",
    "preact-compat": "^3.19.0",
    "react": "16.12.0",
    "react-app-rewired": "^2.1.6",
    "react-dom": "16.12.0",
    "react-scripts": "3.0.1",
    "react-spring": "8.0.27",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.8.2"
  },
  "scripts": {
    "start": "npm run watch:css && react-app-rewired start",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
