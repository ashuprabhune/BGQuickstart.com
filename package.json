{
    "name": "bg-quickstart",
    "version": "1.0.0",
    "description": "The fastest way to pick a start player",
    "main": "src/index.js",
    "dependencies": {
        "@fullhuman/postcss-purgecss": "^2.1.0",
        "@types/lodash-es": "^4.17.3",
        "autoprefixer": "^9.7.4",
        "cssnano": "^4.1.10",
        "framer-motion": "1.8.4",
        "lodash-es": "4.17.15",
        "postcss-cli": "^7.1.0",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-scripts": "3.0.1",
        "react-spring": "8.0.27",
        "tailwindcss": "^1.2.0",
        "typescript": "^3.8.2"
    },
    "scripts": {
        "start": "npm run watch:css && react-scripts start",
        "build": "npm run build:css && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "build:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css --env production",
        "watch:css": "postcss src/css/tailwind.src.css -o src/css/tailwind.css"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/react": "^16.9.46"
    }
}
